{
  "name": "fin-folio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:cashdash": "pnpm --filter cashdash dev",
    "start:cashcore": "pnpm --filter cashcore start",
    "lint:cashcore": "pnpm --filter cashcore exec eslint . --ext .ts,.js --fix",
    "lint:cashdash": "pnpm --filter cashdash exec eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:fix": "pnpm run lint:cashcore && pnpm run lint:cashdash",
    "migrate:db": "pnpm --filter cashcore db:migrate",
    "create:db": "pnpm --filter cashcore db:create"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "mime-types": "^3.0.1",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.38.0"
  }
}
