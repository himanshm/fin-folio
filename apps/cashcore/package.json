{
  "name": "cashcore",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "start": "tsx src/server.ts",
    "start:dev": "tsx watch src/server.ts",
    "script": "tsx src/scripts/start.ts",
    "lint:changed": "tsx lint-staged-lines.ts",
    "db:create": "NODE_OPTIONS='--import tsx' sequelize db:create",
    "db:migrate": "NODE_OPTIONS='--import tsx' sequelize db:migrate",
    "db:model:generate": "NODE_OPTIONS='--import tsx' sequelize model:generate",
    "db:migration:create": "NODE_OPTIONS='--import tsx' sequelize migration:create",
    "db:migrate:undo": "NODE_OPTIONS='--import tsx' sequelize db:migrate:undo",
    "db:migrate:undo:all": "NODE_OPTIONS='--import tsx' sequelize db:migrate:undo:all",
    "db:seed:generate": "NODE_OPTIONS='--import tsx' sequelize seed:generate",
    "db:seed": "NODE_OPTIONS='--import tsx' sequelize db:seed:all",
    "db:seed:undo": "NODE_OPTIONS='--import tsx' sequelize db:seed:undo",
    "db:seed:undo:all": "NODE_OPTIONS='--import tsx' sequelize db:seed:undo:all"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bluebird": "^3.7.2",
    "express": "^5.1.0",
    "sequelize": "^6.37.7",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "@fin-folio/config": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.27.1",
    "@types/babel__register": "^7.17.3",
    "@types/bluebird": "^3.5.42",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  }
}
